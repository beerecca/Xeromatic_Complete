root = true ; top-most EditorConfig file


; Windows-style newlines with 4 space indentation

[*]
indent_style = space
indent_size = 4
end_of_line = crlf

[*.js]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = false

[*.scss]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = false
